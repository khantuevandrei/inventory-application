<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title><%= title %></title>
  </head>
  <body>
    <h1><%= title %></h1>
    <nav>
      <h2>Trainers:</h2>
      <ul>
        <% trainers.forEach((trainer) => { %>
        <li>
          <a href="/trainers/<%= trainer.trainer %>">
            <%= trainer.trainer %>
          </a>
        </li>
        <br />
        <% }); %>
        <li>
          <button><a href="/trainers/create">create trainer</a></button>
        </li>
      </ul>

      <h2>Pokemon types:</h2>
      <ul>
        <% types.forEach((type) => { %>
        <li><a href="/types/<%= type.type %>"> <%= type.type %> </a></li>
        <br />
        <% }); %>
        <li>
          <button><a href="/types/create">create type</a></button>
        </li>
      </ul>
    </nav>
  </body>
</html>
