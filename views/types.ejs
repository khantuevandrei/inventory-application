<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title><%= title %></title>
  </head>
  <body>
    <h1><%= title %>:</h1>
    <nav>
      <ul>
        <% types.forEach((type) => { %>
        <li>
          <a href="/types/<%= type.type %>"> <%= type.type %> </a>
          <form
            action="/types/<%=type.type%>/?_method=DELETE"
            method="POST"
            style="display: inline"
          >
            <button type="submit">delete</button>
          </form>
        </li>
        <br />
        <% }); %>
        <a href="/types/create"><button>create type</button></a>
      </ul>
      <a href="/">Home</a>
    </nav>
  </body>
</html>
